<div class="jogger">
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("X-Y+")'><MudIcon Icon="@Icons.Material.Filled.NorthWest" /></MudButton>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("Y+")'><MudIcon Icon="@Icons.Material.Filled.North" /></MudButton>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("X+Y+")'><MudIcon Icon="@Icons.Material.Filled.NorthEast" /></MudButton>
    <div></div>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("Z+")'><MudIcon Icon="@Icons.Material.Filled.North" /></MudButton>

    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("X-")'><MudIcon Icon="@Icons.Material.Filled.West" /></MudButton>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("X0Y0")'>X0Y0</MudButton>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("X+")'><MudIcon Icon="@Icons.Material.Filled.East" /></MudButton>

    <div></div>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("Z0")'>Z0</MudButton>

    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("X-Y-")'><MudIcon Icon="@Icons.Material.Filled.SouthWest" /></MudButton>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("Y-")'><MudIcon Icon="@Icons.Material.Filled.South" /></MudButton>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("X+Y-")'><MudIcon Icon="@Icons.Material.Filled.SouthEast" /></MudButton>
    
    <div></div>
    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick='() => Jog("Z-")'><MudIcon Icon="@Icons.Material.Filled.South" /></MudButton>
    
